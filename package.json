{
  "name": "@megaflow-labs/sdk",
  "version": "0.1.0",
  "description": "Batch transaction SDK for MegaETH â€” compose, simulate and execute multiple on-chain operations in a single atomic transaction.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "megaeth",
    "megaflow",
    "ethereum",
    "l2",
    "evm",
    "transaction-builder",
    "batch-transactions",
    "atomic",
    "defi",
    "router",
    "web3",
    "viem"
  ],
  "author": "MegaFlow Labs <contact@megaflow.space>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/megaflow-labs/sdk"
  },
  "homepage": "https://megaflow.space",
  "dependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}